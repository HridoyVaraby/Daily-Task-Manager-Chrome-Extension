{
      "manifest_version": 3,
      "name": "Daily Task Manager",
      "version": "1.0.0",
      "description": "A simple task manager extension.",
       "action": {
        "default_popup": "popup.html",
        "default_icon": {
          "16": "task.png",
          "48": "task.png",
          "128": "task.png"
        }
      },
      "icons": {
        "16": "task.png",
        "48": "task.png",
        "128": "task.png"
      },
      "permissions": [
        "storage"
      ]
    }
